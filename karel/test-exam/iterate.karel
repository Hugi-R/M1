BEGINNING-OF-PROGRAM
	BEGINNING-OF-EXECUTION

		RESET beeps0;
		
		COUNT beeps0
			ITERATE 8 TIMES
				move;
		
		turnleft;
		turnleft;
		
    COUNT beeps1
      ITERATE beeps0 TIMES
        move;
		
		turnleft;
		turnleft;
    turnleft;
		
		IF front-is-clear THEN
      ITERATE 2 TIMES
        ITERATE beeps1 TIMES
          move;

    move;
		turnleft;

    move;

		
		turnoff

	END-OF-EXECUTION
END-OF-PROGRAM
