BEGINNING-OF-PROGRAM
  BEGINNING-OF-EXECUTION

    IF not-next-to-a-beeper THEN
      move;
    
    IF not-facing-north THEN
      BEGIN
        turnleft;
      END
    ELSE
        move;
    
    IF not-facing-east THEN
      IF not-facing-west THEN
        turnleft
        ELSE
            IF not-facing-east THEN
                move
            ELSE
                move;
  
    turnoff
  END-OF-EXECUTION
END-OF-PROGRAM